{
  "extends": [
    "config:base"
  ],
  "baseBranches": ["master"],
  "packageRules": [
    {
      "matchDatasources": ["npm"],
      "stabilityDays": 3
    }
  ],
  "automerge": true,
  "automergeType": "branch",
  "prHourlyLimit": 5,
  "prConcurrentLimit": 10,
  "rangeStrategy": "bump",
  "timezone": "America/Chicago",
  "schedule": ["before 5am", "after 7pm"]
}
