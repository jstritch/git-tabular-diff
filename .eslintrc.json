{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jasmine": true,
    "atomtest": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "@babel"
  ],
  "rules": {
    "semi": "error",
    "quotes": ["error", "single", { "allowTemplateLiterals": true }]
  }
}
