{
  "name": "git-tabular-diff",
  "main": "./lib/git-tabular-diff",
  "version": "0.0.0",
  "description": "Displays a tabular diff of csv files in an Atom pane.",
  "keywords": [
    "git",
    "tabular",
    "diff",
    "csv",
    "daff"
  ],
  "activationCommands": {
    "atom-workspace": "git-tabular-diff:compare-selected-files"
  },
  "repository": "https://github.com/jstritch/git-tabular-diff",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "lint": "npm run lint:less & npm run lint:js",
    "lint:js": "npx eslint --ext .js,.json --ignore-path .gitignore .",
    "lint:less": "npx stylelint styles",
    "test": "atom --test spec"
  },
  "dependencies": {
    "daff": "^1.3.44",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/eslint-plugin": "^7.14.5",
    "@babel/preset-env": "^7.15.0",
    "@primer/css": "^17.6.0",
    "eslint": "^7.32.0",
    "eslint-plugin-jsonc": "^1.6.0",
    "stylelint": "^13.13.1",
    "stylelint-config-primer": "^11.0.1",
    "stylelint-config-standard": "^22.0.0"
  }
}
