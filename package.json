{
  "name": "git-tabular-diff",
  "main": "./lib/git-tabular-diff",
  "version": "1.0.1",
  "description": "Displays a tabular difference of csv files in an Atom pane.",
  "keywords": [
    "compare",
    "diff",
    "csv",
    "data",
    "table",
    "tabular",
    "git",
    "daff"
  ],
  "activationCommands": {
    "atom-workspace": "git-tabular-diff:compare-selected-files"
  },
  "deserializers": {
    "GitTabularDiffView": "deserializeGitTabularDiffView"
  },
  "repository": "https://github.com/jstritch/git-tabular-diff",
  "author": "John Stritch",
  "license": "MIT",
  "funding": "https://github.com/sponsors/jstritch",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "atomTestRunner": "./spec/custom-runner",
  "scripts": {
    "lint": "npx eslint --ext .js,.json --ignore-path .gitignore .",
    "test": "atom --test spec"
  },
  "dependencies": {
    "daff": "^1.3.44",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.0",
    "@babel/eslint-plugin": "^7.14.5",
    "@babel/preset-env": "^7.16.0",
    "atom-jasmine3-test-runner": "^5.2.9",
    "eslint": "^8.2.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsonc": "^1.7.0"
  }
}
